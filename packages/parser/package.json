{
  "name": "@markdown-next/parser",
  "version": "1.0.0",
  "description": "Markdown parser for markdown-next",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "markdown",
    "parser"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@markdown-next/shared": "workspace:*",
    "@types/hast": "^3.0.4",
    "rehype-mathjax": "^7.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "tsup": "^8.5.1",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "vitest": "^4.0.16"
  }
}
